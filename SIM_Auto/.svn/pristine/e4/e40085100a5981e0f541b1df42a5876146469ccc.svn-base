	/**
	 * 
	 * UserRoles: Verify that SIM app has Lavante support role running with
	 * UserRoleID=17 , this support role account should be able to see customers
	 * drop down on top, changing to any customers should be possible
	 * 
	 * SIM 7.2 
	 * @author Subhash.BV
	 * @throws Exception
	 */
	@Test
	public void verifyCustomerRoleDropDownTest() throws Exception {
		Reporter.log("Test Started for verifyCustomerDropDownTest : " + currenttime());
		SoftAssert softAssert=new SoftAssert();
		
		lavanteUtils.click(enobjhomePage.AllCustomersDropDwn());
				
		List<String> customers = listelement(null);
		int random = randomnum(customers.size());
		String selctedCust = "";
		selctedCust=customers.get(random) ;
		
		while(!(selctedCust.length()>0)){
			selctedCust=customers.get(random) ;
			random = randomnum(customers.size());
		}
		
		String x="select Count(NAme) From Customer ";
		x=lavanteUtils.fetchDBdata(x);
		List<String> actuals = new ArrayList<String>();
		for(int i=0;i<customers.size();i++){
			if(customers.get(i).length()>0){
				actuals.add(customers.get(i));
			}
		}
		Reporter.log("Customer Count,Exp:"+x+",Actuals:"+actuals.size(),true);
		softAssert.assertEquals(x, ""+actuals.size(),"Customer Count Not matched");
		
		lavanteUtils.typeinEdit(selctedCust, enobjhomePage.AllCustomersDropDwnTxtBox);
		lavanteUtils.selectvalueFrmDpdn(selctedCust);
		Reporter.log("SELECTED CUSTOMER:"+selctedCust,true);
		
		 x = "select CustomerLogoUrl from Customer where [Name]='" + selctedCust + "'";
		 x = lavanteUtils.fetchDBdata(x);
		 
		lavanteUtils.waitForTime(5000);
		String actual = enobjhomePage.customerlogo().getAttribute("src");

		Reporter.log("Verify  support role account should be able to see customers drop down on top, changing to any customers should be possible. Expected, "	+ x + " Actual , " + actual);

		if (actual.contains(x)) {
			softAssert.assertTrue(true,"Verify  support role account should be able to see customers drop down on top, changing to any customers should be possible. Expected, "	+ x + " Actual , " + actual);
		} else {

			softAssert.assertTrue(false,"Verify  support role account should be able to see customers drop down on top, changing to any customers should be possible. Expected, "		+ x + " Actual , " + actual);
		}
		
		lavanteUtils.click(enobjhomePage.RoleproxyDpdn());
		
		customers = listelement(null);
		
		 x="select count(UserRoleID) from CustomerLoginRoleMap where  IsActive=1 and CustomerID in (Select CustomerID from Customer where Name='" + selctedCust + "')";
		x=lavanteUtils.fetchDBdata(x);
		actuals = new ArrayList<String>();
		for(int i=0;i<customers.size();i++){
			if(customers.get(i).length()>0 && !customers.get(i).contains("Act on behalf")){
				actuals.add(customers.get(i));
			}
		}
		Reporter.log("Role Count,Exp:"+x+",Actuals:"+actuals.size(),true);
		softAssert.assertEquals(x, ""+actuals.size(),"Role Count Not matched");
		
		
		random = randomnum(actuals.size());
		selctedCust = "";
		selctedCust=actuals.get(random) ;
		String Role="";
		while(!(selctedCust.length()>0)){
			Role=actuals.get(random) ;
			random = randomnum(actuals.size());
		}
		lavanteUtils.selectvalueFrmDpdn(Role);
		Reporter.log("SELECTED Role:"+Role,true);
			
		
		lavanteUtils.fluentwait(enobjhomePage.supportbannermsg());
		x=enobjhomePage.supportbannermsg().getText().trim();
		Reporter.log("Actuals:"+x+",Expected:"+selctedCust,true);
		softAssert.assertTrue(x.contains(Role),"After Role selection, support banner is not shown");
		softAssert.assertTrue(x.contains(selctedCust),"After Role selection, support banner is not shown");
		
		
		boolean flag=lavanteUtils.isElementDisplayed(enobjhomePage.returntoSupport());
		Reporter.log("After RoleSection ,Support Return msg expected true,actuals:"+flag);
		softAssert.assertTrue(flag,"After Role selection, support banner is not shown");
		
		lavanteUtils.click(enobjhomePage.returntoSupport());
		lavanteUtils.fluentwait(enobjhomePage.UserName());
		
		flag=lavanteUtils.isElementDisplayed(enobjhomePage.returntoSupport());
		Reporter.log("After RoleSection ,Support Return msg expected false,actuals:"+flag);
		softAssert.assertFalse(flag,"After return, support banner is shown");
		
		softAssert.assertAll();
		Reporter.log("Test Ended at:" + currenttime());
	}

