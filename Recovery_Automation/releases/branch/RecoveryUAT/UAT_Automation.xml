			 <class name="com.lavante.recovery.TestScripts.UAT.QuickSearch" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_SetUp_BusinessUnit" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_Finance_ContractDetails" />
			 
			 
