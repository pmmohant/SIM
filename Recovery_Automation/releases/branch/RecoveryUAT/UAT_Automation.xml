<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Lavante -  Recovery Automation UAT Test Suite" parallel="classes" thread-count="3" verbose="1">
		
	 <!-- Running on IE -->
    <test  name="Lavante - Recovery - Running on IE">
		<parameter name="Platform" value="Windows" />
		<parameter name="Browser" value="internet explorer" />
		<parameter name="Version" value="10" />	
		<classes> 
			
			 <class name="com.lavante.recovery.TestScripts.UAT.QuickSearch" />
			 
			<class name="com.lavante.recovery.TestScripts.UAT.Dashboard_Supplier" />
			<class name="com.lavante.recovery.TestScripts.UAT.DashboardCompliance" />
			<class name="com.lavante.recovery.TestScripts.UAT.Dashboard_Claims" />
			
     		 <class name="com.lavante.recovery.TestScripts.UAT.Audit_CheckRegistry" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Audit_ContactEntry" />

			<class name="com.lavante.recovery.TestScripts.UAT.Config_Acct_AuditGroup" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_Acct_User" /> 
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_Acct_UserGroup" /> 
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_CreditRejection" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_FaxComments" /> 
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_PhoneExtension" />
			 <class name="com.lavante.recovery.TestScripts.UAT.ConfigurationMailerRegistry" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_SetUp_BusinessUnit" />
			 <class name="com.lavante.recovery.TestScripts.UAT.Config_Finance_ContractDetails" />
			 
		</classes>
	</test>  
		
